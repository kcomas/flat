<div class="row">
    <div class="col-md-12">
        <form novalidate>
            <div class="form-group">
                Name: <input class="form-control"  type="text" ng-model="current.name" />
            </div>
            <div class="form-group">
                <label class="radio-inline">
                    <input type="radio" ng-model="current.public" ng-value="false"  /> Public
                </label>
                <label class="radio-inline">
                    <input type="radio" ng-model="current.public" ng-value="true" /> Private
                </label>
                <div class="form-group">
                    <input type="file" name="fileData"  id="fileToBeUploaded" class="btn btn-primary" value="Upload" />
                </div>
                <div class="form-group text-center">
                    <button class="btn btn-success" ng-click="upload()">Upload</button>
                    <button class="btn btn-danger" ng-click="clear()">Clear</button>
                </div>
            </div>
        </form>
    </div>
</div>
