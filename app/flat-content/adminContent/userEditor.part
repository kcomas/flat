<div class="row">
    <div class="col-md-12">
        <h2>Hello, {{current.user.username}}</h1> 
        <h5>Sign Up Date {{current.user.dateCreated}}<h5>
        <form novalidate>
            <div class="form-group">
                Email: <input type="email" class="form-control" ng-model="current.user.email" />
            </div>
            <div class="checkbox">
                    <input type="checkbox" ng-model="current.updatePassword" ng-value="true" /> Private
            </div>
        </form>
    </div>
</div>
